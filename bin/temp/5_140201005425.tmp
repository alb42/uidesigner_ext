  //////////// SampleFormat : -1 default : Int16 : (0: Float32, 1:Int32, 2:Int16)
  //////////// FramesCount : -1 default : 65536
  //  result : -1 nothing created, otherwise Output Index in array
  //////////// example : OutputIndex1 := AddIntoFile(edit5.Text,-1,-1,0, -1);
